{
  "parser": "babel-eslint",
  "extends": "eslint:recommended",
  "plugins": [ "react" ],

  "env": {
    "es6": true,
    "browser": true
  },

  "ecmaFeatures": {
    "jsx": true,
    "experimentalObjectRestSpread": true,
    "modules": true
  },
  "rules": {
    "indent": [ 2, 2 ],
    "quotes": [ 2, "single" ],
    "semi": [ 2, "always" ]
  }
}
